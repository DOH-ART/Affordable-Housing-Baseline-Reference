   "source": [
    "import pandas as pd "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "localities = pd.read_csv(r'.\locality_county.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "localities = localities[['geoid','county']]\n",
    "localities = localities.drop_duplicates(subset=['geoid','county'])\n",
    "localities = localities.set_index('geoid')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "locality = {}\n",
    "for idx, rows in localities.iterrows():\n",
    "    if idx in locality:\n",
    "        locality[idx].append(rows['county'])\n",
    "    else:\n",
    "        locality[idx] = []\n",
    "        locality[idx].append(rows['county'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "localities = localities.reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "county_codes = pd.DataFrame()\n",
    "county_codes['geoid'] = list(locality.keys())\n",
    "county_codes['countycode'] = list(locality.values())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 71,
   "metadata": {},
   "outputs": [],
   "source": [
    "county_codes.to_csv(r'.\county_codes.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "acs_data = pd.read_csv(r'.\acs.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 90,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "16700"
      ]
     },
     "execution_count": 90,
